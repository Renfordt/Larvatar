{
  "name": "renfordt/larvatar",
  "description": "A package to distribute you many ways of creating avatars",
  "license": "MIT",
  "authors": [
    {
      "name": "Jannik Renfordt",
      "email": "jannik.renfordt@ikobb.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Renfordt\\Larvatar\\": "src/"
    }
